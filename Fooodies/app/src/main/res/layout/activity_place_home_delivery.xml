<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="in.pulkitpahwa.fooodies.PlaceHomeDelivery">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Order placed. you will receive order at your place in 30 minutes. kepp your change ready"
        android:id="@+id/textView24"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue Ordering"
        android:id="@+id/button11"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:onClick="goToCourses" />
</RelativeLayout>
